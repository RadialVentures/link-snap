  {
  "manifest_version": 3,
  "name": "Link Snap",
  "version": "1.0.0",
  "description": "Snap LinkedIn profiles.",
  "permissions": [
    "tabs", "scripting", "notifications", "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "host_permissions": [
    "https://vqwcdrtnnnykkuaxuaqd.supabase.co/*",
    "*://*.linkedin.com/*",
    "<all_urls>"
  ],
  "web_accessible_resources": [{
    "resources": ["onboarding.html", "onboarding.js", "supabaseClient.js"],
    "matches": ["<all_urls>"]
  }],
  "background": {
    "service_worker": "background.js"
  }
}
